    <validationRules>
        <fullName>AE_Qualification_Completion</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
  ISPICKVAL( StageName , &quot;01 - Discovery&quot;),
   ISPICKVAL( StageName , &quot;02 - Business Validation&quot;),
    ISPICKVAL( StageName , &quot;03 - Solution Validation&quot;),
     ISPICKVAL( StageName , &quot;04 - Manage to Close&quot;),
      ISPICKVAL( StageName , &quot;05a - Closed Won&quot;))

&amp;&amp; 

OR(
ISBLANK( AE_Accepted_Date__c ),
NOT( ISPICKVAL( Initial_Meeting_Result__c ,&quot;Completed&quot;)),
Qualification_Score_AE__c =0)

&amp;&amp;

 
ISPICKVAL( Type , &quot;New&quot;)</errorConditionFormula>
        <errorMessage>All opportunities that progress past qualification stage must be accepted by the AE, have completed an initial meeting, and have AE qualification section completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_Rules</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
((IsClosed = TRUE) &amp;&amp; ( Days_Until_Close__c &gt;0)),
((IsClosed = FALSE) &amp;&amp; ( Days_Until_Close__c &lt; 0))
)</errorConditionFormula>
        <errorMessage>Open: Close Date = estimated close date (must be in future). Won: Close Date = final contract signature date (must be today or in past). Lost: Close Date = when &quot;No&quot; was received (must be today or in past).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_details_validation</fullName>
        <active>true</active>
        <description>Ensure that contract start and end/renewal dates are included upon closed/won</description>
        <errorConditionFormula>IsWon = True 
&amp;&amp; 
OR(
ISBLANK( Contract_Start_Date__c ), 
ISBLANK( Contract_End_Date__c ),
ISBLANK( Amount ),
ISPICKVAL( Payment_Schedule__c ,&quot;&quot;) ,
ISBLANK( Contract_Signatory_Name__c ),
ISBLANK( Billing_Contact_Name__c ),
ISBLANK( Handoff_to_Customer_Success_Date__c )
)</errorConditionFormula>
        <errorMessage>Must include contract, billing/payment, and handoff info before closing a won opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Follow_Up_Date_for_Future_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(
$RecordType.Name = &quot;New or Add-On&quot;,$RecordType.Name = &quot;Renewal or Renewal + Add-On&quot;), If(ISPICKVAL( Lost_Reason__c ,&apos;Timing - Future Opportunity&apos;)&amp;&amp; Follow_Up_Date__c  &lt; TODAY()+1,TRUE,FALSE))</errorConditionFormula>
        <errorDisplayField>Follow_Up_Date__c</errorDisplayField>
        <errorMessage>Follow Up Date should be future date!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Follow_Up_Date_for_Lost_Reson</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(
$RecordType.Name = &quot;New or Add-On&quot;,$RecordType.Name = &quot;Renewal or Renewal + Add-On&quot;),If(ISPICKVAL(  Lost_Reason__c  ,&apos;Timing - Future Opportunity&apos;)&amp;&amp; ISBLANK( Follow_Up_Date__c ),TRUE,FALSE))</errorConditionFormula>
        <errorDisplayField>Follow_Up_Date__c</errorDisplayField>
        <errorMessage>Follow Up Date is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Initial_Meeting_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Initial_Meeting_Date__c ) &amp;&amp;  
ISPICKVAL( Initial_Meeting_Result__c , &quot;Scheduled&quot;) &amp;&amp;
ISPICKVAL( Type , &quot;New&quot;)</errorConditionFormula>
        <errorMessage>Initial Meeting Date cannot be blank when Initial Meeting Result is &quot;Scheduled&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Initial_Meeting_Result</fullName>
        <active>true</active>
        <errorConditionFormula>(TODAY() &gt;  Initial_Meeting_Date__c) &amp;&amp;  
ISPICKVAL( Initial_Meeting_Result__c , &quot;&quot;) &amp;&amp;
ISPICKVAL( Type , &quot;New&quot;)</errorConditionFormula>
        <errorDisplayField>Account_Dev_Owner_User__c</errorDisplayField>
        <errorMessage>Enter disposition for Initial Meeting Results or update Initial Meeting Date to the newly scheduled time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LID__Check_CompanyId_Value</fullName>
        <active>true</active>
        <description>Throws an exception message if the company id contains non-numerics</description>
        <errorConditionFormula>AND(
	(LID__LinkedIn_Company_Id__c &lt;&gt; NULL),
	NOT(ISNUMBER(LID__LinkedIn_Company_Id__c))
)</errorConditionFormula>
        <errorDisplayField>LID__LinkedIn_Company_Id__c</errorDisplayField>
        <errorMessage>CompanyId should be a numeric value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Class_Cannot_be_Changed</fullName>
        <active>true</active>
        <description>Lead Class cannot be changed once it has been set upon creation.</description>
        <errorConditionFormula>AND(
$Profile.Id &lt;&gt; &quot;00e37000000gogC&quot;,(ISCHANGED(Opportunity_Inbound_Outbound__c))
)</errorConditionFormula>
        <errorMessage>Lead Class cannot be changed, once it is set upon Opportunity creation. Please contact your system administrator to change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Reason_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( StageName ,&quot;05b - Closed Lost&quot;)&amp;&amp; 
OR(
ISPICKVAL( Lost_Reason__c , &quot;&quot;),
ISBLANK( Lost_Reason_Detail__c ))</errorConditionFormula>
        <errorDisplayField>Lost_Reason__c</errorDisplayField>
        <errorMessage>Lost Reason and Lost Reason Detail are required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Standard_Payment_Details</fullName>
        <active>true</active>
        <description>Ensure that non-standard payment schedule details are included with opportunity</description>
        <errorConditionFormula>ISPICKVAL( Payment_Schedule__c , &quot;Other&quot;) &amp;&amp; ISBLANK( Non_Standard_Financial_Terms_Details__c )</errorConditionFormula>
        <errorMessage>Must include details of all non-standard payment schedules</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Progression_From_Stage_01</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
(Pain_Score__c  = 0),
(Champion_Score__c = 0),
(Competitors_Score__c = 0)
)

&amp;&amp;

OR(
(ISPICKVAL(StageName, &quot;02 - Business Validation&quot;)),
(ISPICKVAL(StageName, &quot;03 - Solution Validation&quot;)),
(ISPICKVAL(StageName, &quot;04 - Manage to Close&quot;)),
(ISPICKVAL(StageName, &quot;05a - Closed Won&quot;))
)

&amp;&amp;

  CloseDate &gt;= DATE(2018,07,01)

&amp;&amp;
  ISPICKVAL(Type, &quot;New&quot;)</errorConditionFormula>
        <errorMessage>Sales methodology sections cannot be left blank when moving to next stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Progression_From_Stage_02</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
( Economic_Buyer_Score__c = 0),
( Decision_Process_Score__c = 0),
( Decision_Criteria_Score__c = 0)
)

&amp;&amp;

OR(
(ISPICKVAL(StageName, &quot;03 - Solution Validation&quot;)),
(ISPICKVAL(StageName, &quot;04 - Manage to Close&quot;)),
(ISPICKVAL(StageName, &quot;05a - Closed Won&quot;))
)

&amp;&amp;

  CloseDate &gt;= DATE(2018,07,01)

&amp;&amp;
  ISPICKVAL(Type, &quot;New&quot;)</errorConditionFormula>
        <errorMessage>Sales methodology sections cannot be left blank when moving to next stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Progression_From_Stage_03</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
(Proof_of_Life_Score__c = 0),
(Commit_Score__c = 0)
)

&amp;&amp;

OR(
(ISPICKVAL(StageName, &quot;04 - Manage to Close&quot;)),
(ISPICKVAL(StageName, &quot;05a - Closed Won&quot;))
)

&amp;&amp;

  CloseDate &gt;= DATE(2018,07,01)

&amp;&amp;
  ISPICKVAL(Type, &quot;New&quot;)</errorConditionFormula>
        <errorMessage>Sales methodology sections cannot be left blank when moving to next stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Contact_Roles_if_Pipeline</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(ISPICKVAL(  StageName, &quot;00 - Discovery&quot;)) &amp;&amp;   (Primary_Contact_Assigned__c = FALSE)</errorConditionFormula>
        <errorMessage>Primary Contact Must Be Assigned to the Opportunity. Please identify Primary Contact prior to moving ahead in the sales process.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Amount_for_Pipeline</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
ISPICKVAL( StageName , &quot;03 - Solution Validation&quot;),
ISPICKVAL( StageName , &quot;04 - Manage to Close&quot;))  
&amp;&amp;  
ISBLANK(Amount)</errorConditionFormula>
        <errorMessage>Estimate of opportunity amount must be included prior to any solution evaluation.</errorMessage>
    </validationRules>
