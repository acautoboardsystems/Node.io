        <fullName>Click_To_Call</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Click To Call</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!RequireScript(&quot;/soap/ajax/29.0/connection.js&quot;)}
var contactRecords= {!GETRECORDIDS($ObjectType.Contact)};
console.log(contactRecords);
var csvContactIds = &quot;&quot;;
for(var rowNum in contactRecords)
{
console.log(contactRecords[rowNum]);
  csvContactIds += &quot;&apos;&quot; + contactRecords[rowNum] + &quot;&apos;,&quot;;
console.log(csvContactIds);
}
//Remove last comma
csvContactIds = csvContactIds.slice(0, csvContactIds.length - 1);

//Enclose the ids in round brackets
if(csvContactIds.length &gt; 1){
    csvContactIds = &quot;(&quot; + csvContactIds + &quot;)&quot;;
}
var username = sforce.connection.getUserInfo().userName;
var contactStatus = &quot; &quot;;
if (contactRecords.length ==0) {
    alert(&quot;Please select at least one Contact record.&quot;);
}
else {
    var result = sforce.connection.query(&quot;select Name, Id, Email, Account.Name, Phone, MailingCountry, Title from Contact Where Id IN &quot; +csvContactIds);
    var records = result.getArray(&quot;records&quot;);
    
    var status = &quot;&quot;;
    for (var i=0; i&lt; records.length; i++) {
        status += records[i].Company+ &apos;\n&apos;;
records[i] = String(records[i]).substring(0, String(records[i]).length-3) + &quot;}&quot;;
}
var iframeData = {};
iframeData[&apos;records&apos;] = &apos;[&apos; + records + &apos;]&apos;;
iframeData[&apos;object&apos;] = &apos;Contact&apos;;
iframeData[&apos;message&apos;] = &apos;NativeSFDC&apos;;
var frames = document.getElementsByTagName(&apos;iframe&apos;);
var ourframe;
for (i = 0; i &lt; frames.length; ++i)
{
    if(frames[i].title == &apos;ConnectAndSellWidget&apos;){
        ourframe = frames[i];
        break;
    }
}
ourframe.contentWindow.postMessage(iframeData, &apos;*&apos;);
}</url>
    </webLinks>
